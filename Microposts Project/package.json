        "@babel/preset-env": "^7.4.3",
        "@babel/preset-stage-0": "^7.0.0",
        "webpack": "^4.16.1",
        "webpack-cli": "^3.0.8",
        "webpack-dev-server": "^3.1.10"
    }
}